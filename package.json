{
  "name": "zcj-vite-vu-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:uat": "vue-tsc --noEmit && vite build --mode uat",
    "preview": "vite preview",
    "prepare": "husky install && yarn commitmsg",
    "commitmsg": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.2",
    "axios": "^0.27.2",
    "husky": "7.0.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^17.0.30",
    "@vitejs/plugin-vue": "^2.3.1",
    "commitlint": "^17.0.2",
    "less": "^4.1.2",
    "lint-staged": "^11.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": [
      "eslint --cache",
      "git add"
    ]
  }
}
