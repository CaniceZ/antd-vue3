{
  "name": "zcj-vite-vu-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:uat": "vite build --mode uat",
    "preview": "vite preview",
    "start": "http-server ./dist -p 3109 --cors",
    "prepare": "yarn rm-husky-config && husky install && yarn precommit && yarn commitmsg",
    "commitmsg": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit \"$1\"'",
    "precommit": "npx husky add .husky/pre-commit 'npx lint-staged'",
    "rm-husky-config": "rimraf .husky",
    "lint-eslint": "eslint \"*.{js,jsx,ts,tsx,vue}\" --fix && prettier \"*.{js,jsx,ts,tsx,vue}\" --write ."
  },
  "dependencies": {
    "ant-design-vue": "^3.2.2",
    "axios": "^0.27.2",
    "eslint": "^8.17.0",
    "http-server": "^14.1.1",
    "husky": "7.0.0",
    "rimraf": "^3.0.2",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "commitlint": "^17.0.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "less": "^4.1.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
