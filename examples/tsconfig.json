{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "lib": ["esnext", "dom"],
    "types": ["vite/client"],
    "plugins": [{ "name": "@vuedx/typescript-plugin-vue" }],
    "paths": {
      "@ygp/ygp-design-vue": ["../src/index.ts"],
      "@ygp/ygp-design-vue/hooks": ["../src/hooks/index.ts"],
      "@ygp/ygp-design-vue/http": ["../src/http/index.ts"],
      "@ygp/ygp-design-vue/utils": ["../src/utils/index.ts"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/shims.d.ts",
    "../node_modules/vue/macros-global.d.ts",
    "../node_modules/ant-design-vue/typings/global.d.ts"
  ]
}
